            <img src="<%=Url.Content("~/images/desktop/preview/ch1/p074.jpg")  %>"
        </td>
        <td>
            <img src="<%=Url.Content("~/images/desktop/preview/ch1/p075.jpg")  %>"
        </td>
        <td ></td>
    </tr>

